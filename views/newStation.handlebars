<div class="container d-flex flex-column">
  <div class="title d-flex flex-column align-items-center px-5 pt-3 pb-2">
    <h2 id="user_info" data-userId={{user_id}}>Register Your Station Here!</h2>
  </div><form class="newStation-form d-flex flex-column align-items-center">
      <div class="form-group d-flex flex-column align-items-center">
        <label for="Station_name">Station Name :</label>
        <input class="form-input" type="text" id="Station_name" />
      </div>
      <div class="form-group d-flex flex-column align-items-center">
        <label for="station_address">Station Address :</label>
        <input class="form-input" type="text" id="station_address" />
      </div>
      <div class="form-group d-flex flex-column align-items-center">
        <label for="Station_city">Station City :</label>
        <input class="form-input" type="text" id="Station_city" />
      </div>
      <div class="form-group d-flex flex-column align-items-center">
        <label for="Station_state">Station State :</label>
        <input class="form-input" type="text" id="Station_state" />
      </div>
      <div class="form-group d-flex flex-column align-items-center">
        <label for="Station_zip">Station Zip Code :</label>
        <input class="form-input" type="text" id="Station_zip" />
      </div>
      <div class="form-group d-flex flex-column align-items-center">
        <label for="station_type">Station type :</label>
        <select name="station_type" id="station_type">
            <option value=“Level-1”>Level 1 (120-Volt)</option>
            <option value=“Level-2”>Level 2 (240-Volt)</option>
            <option value=“DC-FastCharging”>DC-FastCharge</option>
        </select>
      </div>
      <div class="form-group d-flex flex-column align-items-center">
        <label for="plug_in_type">Plug-in types :</label>
        <select name="plug_in_type" id="plug_in_type">
          <option value="J1772"> J1772 Connector</option>
          <option value="Tesla"> Tesla Connector</option>
          <option value="CCS"> CCS Connector</option>
          <option value="CHAdeMO"> CHAdeMO Connector</option>
        </select>
      </div>
      {{!-- <div class="form-group d-flex flex-column align-items-center">
        <label for="availability-register" class="form-drop">Station Availability :</label>
        <input class="form-drop" type="text" id="availability-register" />
      </div> --}}
      {{!-- <div class="form-group">
        <label for="price-register">Station-Price :</label>
        <input class="form-input" type="currency" id="price-register" />
      </div> --}}
      <div class="form-group d-flex flex-column align-items-center my-3">
        <button class="btn btn-primary" type="submit">Register Your Station</button>
      </div>
    </form>
  </div>
</div>
