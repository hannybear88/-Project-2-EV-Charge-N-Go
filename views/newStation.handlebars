<div class="container d-flex flex-column mb-5">
  <!-- title -->  
  <div class="title d-flex flex-column align-items-center px-5 pt-3 pb-2">
    <div id="user-info" data-user-id={{user_id}}></div>
    <h2>Register Your Station Here!</h2>
  </div>
  <!-- form -->
  <form class="newStation-form p-3">
    <!-- station name -->
    <div class="mb-4">
      <label for="station-name" class="form-label">Station Name :</label>
      <input class="form-control" type="text" id="station-name" placeholder="My Station #1" />
    </div>
    <!-- station address -->
    <div class="mb-4">
      <label for="station-address" class="form-label">Station Address :</label>
      <div class="row mb-1">
        <div class="col">
          <input class="form-control" type="text" id="station-address" placeholder="Street addresss" />
        </div>
      </div>
      <div class="row">
        <div class="col-6">
          <input class="form-control" type="text" id="station-city" placeholder="City" />
        </div>
        <div class="col-2">
          <input class="form-control" type="text" id="station-state" placeholder="State" />
        </div>
        <div class="col-4">
          <input class="form-control" type="text" id="station-zip" placeholder="Zip" />
        </div>
      </div>
    </div>
    <!-- station type -->



    <div class="mb-4">
      <label for="station-type">Station type :</label>
      <select class="form-select" name="station-type" id="station-type">
        <option value="Level-1">Level 1 (120-Volt)</option>
        <option value="Level-2">Level 2 (240-Volt)</option>
        <option value="DC-FastCharging">DC-FastCharge</option>
      </select>
    </div>
    <div class="mb-4">
      <label for="plug-in-type">Plug-in types :</label>
      <select class="form-select" name="plug-in-type" id="plug-in-type">
        <option value="J1772"> J1772 Connector</option>
        <option value="Tesla"> Tesla Connector</option>
        <option value="CCS"> CCS Connector</option>
        <option value="CHAdeMO"> CHAdeMO Connector</option>
      </select>
    </div>
    <div class="form-group d-flex flex-column align-items-center my-3">
      <button class="btn btn-primary" type="submit">Register Your Station</button>
    </div>
  </form>
</div>
</div>
<script src="/js/newStation.js"></script>
